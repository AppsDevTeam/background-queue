{
    "name": "adt/background-queue",
    "type": "library",
    "description": "Background queue for Nette Framework",
    "keywords": [
        "Nette",
        "RabbitMQ",
        "Symfony Messenger",
        "Laravel Queues"
    ],
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Apps Dev Team",
            "homepage": "https://www.appsdevteam.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^4.0",
        "symfony/console": "^4.0|^5.0|^6.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "adt/utils": "^2.10",
        "adt/command-lock": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ADT\\BackgroundQueue\\": "src/"
        }
    },
    "require-dev": {
        "ext-bcmath": "*",
        "ext-sockets": "*",
        "codeception/codeception": "^5.0",
        "php-amqplib/php-amqplib": "^2.8",
        "codeception/module-asserts": "^2.0",
        "adt/docker-binaries": "^3.6",
        "codeception/assert-throws": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://git.appsdevteam.com/adt/docker-binaries.git",
            "no-api": true
        }
    ]
}
