{
    "name": "adt/background-queue",
    "type": "library",
    "description": "Background queue for Nette Framework",
    "keywords": [
        "Nette",
        "RabbitMQ",
        "Symfony Messenger",
        "Laravel Queues"
    ],
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Apps Dev Team",
            "homepage": "https://www.appsdevteam.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.0|^3.0",
        "symfony/console": "^4.0|^5.0|^6.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "adt/utils": "^2.10",
        "adt/command-lock": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ADT\\BackgroundQueue\\": "src/"
        }
    },
    "require-dev": {
        "ext-bcmath": "*",
        "ext-sockets": "*",
        "codeception/codeception": "^4.2",
        "php-amqplib/php-amqplib": "^2.8",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^2.0",
        "adt/docker-binaries": "^3.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Integration\\": "tests/integration/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://git.appsdevteam.com/adt/docker-binaries.git",
            "no-api": true
        }
    ]
}
